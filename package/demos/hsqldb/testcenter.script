CREATE CACHED TABLE EXAM(EXAM_ID INTEGER NOT NULL,DESCRIPTION VARCHAR(255),EXAM_CATEGORY_ID INTEGER,NME VARCHAR(255),JDO_VERSION SMALLINT NOT NULL,CONSTRAINT PK_EXAM PRIMARY KEY(EXAM_ID))
CREATE CACHED TABLE EXAM_CATEGORY(EXAM_CATEGORY_ID INTEGER NOT NULL,DESCRIPTION VARCHAR(255),NME VARCHAR(255),JDO_VERSION SMALLINT NOT NULL,CONSTRAINT PK_EXAM_CATEGORY PRIMARY KEY(EXAM_CATEGORY_ID))
CREATE CACHED TABLE JDO_KEYGEN(TABLE_NAME VARCHAR(64) NOT NULL,LAST_USED_ID INTEGER NOT NULL,CONSTRAINT PK_JDO_KEYGEN PRIMARY KEY(TABLE_NAME))
CREATE CACHED TABLE SYSTEM_USER(SYSTEM_USER_ID INTEGER NOT NULL,JDO_CLASS INTEGER NOT NULL,FIRST_NAME VARCHAR(255),LOGIN VARCHAR(255),PASSWD VARCHAR(255),SURNAME VARCHAR(255),JDO_VERSION SMALLINT NOT NULL,CONSTRAINT PK_SYSTEM_USER PRIMARY KEY(SYSTEM_USER_ID))
CREATE UNIQUE INDEX IDX_SYSTEM_USER_LOGIN ON SYSTEM_USER(LOGIN)
CREATE CACHED TABLE SYSTEM_USER_EXAM(SYSTEM_USER_ID INTEGER NOT NULL,EXAM_ID INTEGER NOT NULL,CONSTRAINT PK_SYSTEM_USER_EXAM PRIMARY KEY(SYSTEM_USER_ID,EXAM_ID))
