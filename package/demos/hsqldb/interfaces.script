CREATE CACHED TABLE ADDRESS(ADDRESS_ID INTEGER NOT NULL,CITY VARCHAR(255),OWNR_CLASS INTEGER,OWNR_ID INTEGER,STREET VARCHAR(255),JDO_VERSION SMALLINT NOT NULL,CONSTRAINT PK_ADDRESS PRIMARY KEY(ADDRESS_ID))
CREATE CACHED TABLE JDO_KEYGEN(TABLE_NAME VARCHAR(64) NOT NULL,LAST_USED_ID INTEGER NOT NULL,CONSTRAINT PK_JDO_KEYGEN PRIMARY KEY(TABLE_NAME))
CREATE CACHED TABLE RESTURANT(RESTURANT_ID INTEGER NOT NULL,ADDRESS_ID INTEGER,NME VARCHAR(255),JDO_VERSION SMALLINT NOT NULL,CONSTRAINT PK_RESTURANT PRIMARY KEY(RESTURANT_ID))
CREATE CACHED TABLE SUPPLIER(SUPPLIER_ID INTEGER NOT NULL,ADDRESS_ID INTEGER,NME VARCHAR(255),JDO_VERSION SMALLINT NOT NULL,CONSTRAINT PK_SUPPLIER PRIMARY KEY(SUPPLIER_ID))
CREATE USER SA PASSWORD "" ADMIN
