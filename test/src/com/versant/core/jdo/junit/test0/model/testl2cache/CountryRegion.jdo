<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jdo
	PUBLIC "-//Sun Microsystems, Inc.//DTD Java Data Objects Metadata 1.0//EN"
	"http://java.sun.com/dtd/jdo_1_0.dtd">
<jdo>
    <package name="com.versant.core.jdo.junit.test0.model.testl2cache">
        <class name="CountryRegion">
            <extension vendor-name="kodo" key="detachable" value="true"/>
            <field name="country">
                <extension vendor-name="kodo" key="fetch-group" value="countryRegion.country"/>
            </field>

            <extension vendor-name="versant" key="fetch-group" value="countryRegion.country">
                <extension vendor-name="versant" key="field-name" value="code">
                </extension>
                <extension vendor-name="versant" key="field-name" value="country">
                </extension>
            </extension>

        </class>
    </package>
</jdo>