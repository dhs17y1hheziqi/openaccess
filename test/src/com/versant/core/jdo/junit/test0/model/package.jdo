                <extension vendor-name="versant" key="field-name" value="accounts" />
                <extension vendor-name="versant" key="field-name" value="bank">
                    <extension vendor-name="versant" key="next-fetch-group" value="bankInfo" />
                    <extension vendor-name="versant" key="jdbc-use-join" value="outer" />
                </extension>
            </extension>
            <query name="clientsWithTransactionsAbove">
                <filter><![CDATA[accounts.contains(acc)
&& acc.transactions.contains(tx)
&& tx.amount > anAmount]]></filter>
                <declare variables="Account acc; Transaction tx" parameters="java.math.BigDecimal anAmount" />
                <extension vendor-name="versant" key="query-param-values">
                    <extension vendor-name="versant" key="value" value="0" />
                </extension>
                <extension vendor-name="versant" key="fetch-group" value="clientTransactions" />
            </query>
            <extension vendor-name="versant" key="fetch-group" value="clientTransactions" />
            <field name="bank" default-fetch-group="true" />
