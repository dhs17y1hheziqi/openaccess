            <!--            <extension vendor-name="versant" key="jdbc-index" value="tx">-->
            <!--                <extension vendor-name="versant" key="field-name" value="transactionId" />-->
            <!--                <extension vendor-name="versant" key="jdbc-unique" value="true" />-->
            <!--            </extension>-->
            <extension vendor-name="versant" key="jdbc-index" value="dateindex">
                <extension vendor-name="versant" key="field-name" value="date" />
            </extension>
            <extension vendor-name="versant" key="jdbc-index" value="payerAccountindex">
                <extension vendor-name="versant" key="field-name" value="payerAccount" />
            </extension>
            <query name="clientsByTotalTxAmount">
                <result grouping="payerAccount.owner"><![CDATA[payerAccount.owner, sum(amount)]]></result>
                <extension vendor-name="versant" key="fetch-group" value="clientInfo" />
            </query>
            <extension vendor-name="versant" key="fetch-group" value="payerAccountOwner">
                <extension vendor-name="versant" key="field-name" value="payerAccount">
                    <extension vendor-name="versant" key="next-fetch-group" value="clientInfo" />
                </extension>
            </extension>
            <extension vendor-name="versant" key="fetch-group" value="clientTransactions">
                <extension vendor-name="versant" key="field-name" value="amount" />
                <extension vendor-name="versant" key="field-name" value="date" />
                <extension vendor-name="versant" key="field-name" value="payerAccount">
                    <extension vendor-name="versant" key="jdbc-use-join" value="outer" />
                    <extension vendor-name="versant" key="next-fetch-group" value="accountInfo" />
                </extension>
                <extension vendor-name="versant" key="field-name" value="recipientAccount">
                    <extension vendor-name="versant" key="jdbc-use-join" value="outer" />
                    <extension vendor-name="versant" key="next-fetch-group" value="accountInfo" />
                </extension>
                <extension vendor-name="versant" key="field-name" value="transactionId" />
            </extension>
            <query name="allClientTransactions">
                <filter><![CDATA[payerAccount.owner.person.firstName == 'John']]></filter>
                <extension vendor-name="versant" key="fetch-group" value="clientTransactions" />
                <declare parameters="Client c" />
                <extension vendor-name="versant" key="query-param-values">
                    <extension vendor-name="versant" key="value" />
                </extension>
            </query>
            <extension vendor-name="versant" key="fetch-group" value="clientInfo">
                <extension vendor-name="versant" key="field-name" value="payerAccount">
                    <extension vendor-name="versant" key="next-fetch-group" value="clientInfo" />
                    <extension vendor-name="versant" key="jdbc-use-join" value="inner" />
                </extension>
            </extension>
            <extension vendor-name="versant" key="jdbc-index" value="recipientAccount">
                <extension vendor-name="versant" key="field-name" value="recipientAccount" />
            </extension>
            <query name="inlineSQL">
                <filter><![CDATA[sql("$1 in (select transaction_id from trnsaction t, account a, bank b, person p, client c, employee e where t.payer_account_account_id = a.account_id and a.company_id = b.company_id and b.company_id = c.client_id      and p.person_id = c.person_id and c.person_id = e.person_id)")]]></filter>
            </query>
